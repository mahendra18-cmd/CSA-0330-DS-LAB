#include <stdio.h>
#include <ctype.h>

char stack[50];
int top = -1;

void push(char c) { stack[++top] = c; }
char pop() { return stack[top--]; }

int precedence(char c) {
    if(c=='+' || c=='-') return 1;
    if(c=='*' || c=='/') return 2;
    return 0;
}

int main() {
    char exp[50], x;
    printf("Enter infix expression: ");
    scanf("%s",exp);

    for(int i=0;exp[i]!='\0';i++) {
        if(isalnum(exp[i]))
            printf("%c",exp[i]);
        else {
            while(top!=-1 && precedence(stack[top])>=precedence(exp[i])) {
                x = pop();
                printf("%c",x);
            }
            push(exp[i]);
        }
    }
    while(top!=-1)
        printf("%c",pop());

    return 0;
}
