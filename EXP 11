#include <stdio.h>
#define SIZE 5

int stack[SIZE], top = -1;

void push(int val) {
    if(top == SIZE-1)
        printf("Stack Overflow\n");
    else
        stack[++top] = val;
}

void pop() {
    if(top == -1)
        printf("Stack Underflow\n");
    else
        printf("Popped: %d\n", stack[top--]);
}

void peek() {
    if(top == -1)
        printf("Stack empty\n");
    else
        printf("Top element: %d\n", stack[top]);
}

int main() {
    push(10);
    push(20);
    peek();
    pop();
    peek();
    return 0;
}
