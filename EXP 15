#include <stdio.h>
#define SIZE 10

int hash[SIZE];

int main() {
    int n, key, index;

    for(int i=0;i<SIZE;i++)
        hash[i] = -1;

    printf("Enter number of keys: ");
    scanf("%d",&n);

    for(int i=0;i<n;i++) {
        scanf("%d",&key);
        index = key % SIZE;

        while(hash[index] != -1)
            index = (index + 1) % SIZE;

        hash[index] = key;
    }

    for(int i=0;i<SIZE;i++)
        printf("%d -> %d\n", i, hash[i]);

    return 0;
}
